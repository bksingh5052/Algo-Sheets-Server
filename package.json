{
    "name": "leet-lab",
    "version": "1.0.0",
    "type": "module",
    "main": "src/server.js",
    "scripts": {
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "formate:check": "prettier . --check",
        "formate:fix": "prettier . --write",
        "dev": "cross-env NODE_ENV=development nodemon --legacy-watch src/server.js",
        "start": "cross-env NODE_ENV=production node src/server.js",
        "prepare": "husky",
        "prisma:generate:dev": "cross-env NODE_ENV=development prisma generate",
        "prisma:generate:prod": "cross-env NODE_ENV=production prisma generate",
        "prisma:migrate:dev": "cross-env NODE_ENV=development prisma migrate dev",
        "prisma:migrate:prod": "cross-env NODE_ENV=production prisma migrate deploy"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "lint-staged": {
        "*.js": [
            "npm run lint:fix",
            "npm run formate:fix"
        ]
    },
    "description": "",
    "devDependencies": {
        "@commitlint/cli": "^19.5.0",
        "@commitlint/config-conventional": "^19.5.0",
        "@eslint/js": "^9.11.1",
        "eslint": "^9.11.1",
        "eslint-config-prettier": "^9.1.0",
        "globals": "^15.9.0",
        "husky": "^9.1.6",
        "lint-staged": "^15.2.10",
        "nodemon": "^3.1.7",
        "prettier": "3.3.3"
    },
    "dependencies": {
        "@prisma/client": "^6.6.0",
        "axios": "^1.9.0",
        "bcryptjs": "^3.0.2",
        "colorette": "^2.0.20",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dotenv-flow": "^4.1.0",
        "express": "^4.21.0",
        "express-validator": "^7.2.1",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^8.6.3",
        "otp-generator": "^4.0.1",
        "path": "^0.12.7",
        "prisma": "^6.6.0",
        "rate-limiter-flexible": "^5.0.3",
        "resend": "^4.4.1",
        "winston": "^3.14.2",
        "winston-mongodb": "^6.0.0"
    }
}
